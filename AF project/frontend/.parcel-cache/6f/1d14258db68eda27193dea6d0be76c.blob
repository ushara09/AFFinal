.reg-outer-box {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-content: space-between;
  align-items: center;
}

.reg-inner-box {
  width: 40%;
  height: 500px;
  background-color: #d3f1f8;
  margin: 50px auto;
  border-radius: 10px;
}

.img1 {
  background: url("238dffa5a43924f501c5d4c8ea4bba06");
  width: 100%;
  height: 160px;
  object-fit: contain;
}

.img2 {
  background: url("b416ed22896435f2b93f50e230d7f2dd");
  width: 100%;
  height: 160px;
  object-fit: cover;
}

.card-header {
  background-color: white;
  border-style: solid;
  border-width: 2px;
  border-color: #b7b1b1;
  border-radius: 10px;
  border-bottom: none;
}

.reg-card {
  border: none;
  outline: none;
  width: 75%;
  height: 600px;
}

.reg-card button {
  float: bottom;
  width: 100%;
}

.card-body {
  height: 100%;
}

.header-name-box {
  text-align: center;
  margin-top: 30px;
}

.header-name-box h1 {
  color: black;
}
